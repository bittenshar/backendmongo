            "body": {
              "mode": "raw",
              "raw": "{\n  \"overrideReason\": \"Customer provided manual ID verification\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/registrations/{{registrationId}}/admin/override-ticket",
              "host": [
                "{{baseUrl}}"
              ],
              "path": [
                "registrations",
                "{{registrationId}}",
                "admin",
                "override-ticket"
              ]
            }
          }
        },
        {
          "name": "Review Verification Failure - Approve",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              },
              {
                "key": "Authorization",
                "value": "Bearer {{adminToken}}"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"action\": \"approve\",\n  \"reason\": \"Customer verified manually by support team\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/registrations/{{registrationId}}/admin/review-failure",
              "host": [
                "{{baseUrl}}"
              ],
              "path": [
                "registrations",
                "{{registrationId}}",
                "admin",
                "review-failure"
              ]
            }
          }
        },
        {
          "name": "Review Verification Failure - Reject",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              },
              {
                "key": "Authorization",
                "value": "Bearer {{adminToken}}"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"action\": \"reject\",\n  \"reason\": \"Unable to verify identity with multiple attempts\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/registrations/{{registrationId}}/admin/review-failure",
              "host": [
                "{{baseUrl}}"
              ],
              "path": [
                "registrations",
                "{{registrationId}}",
                "admin",
                "review-failure"
              ]
            }
          }
        },
        {
          "name": "Review Verification Failure - Request Retry",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              },
              {
                "key": "Authorization",
                "value": "Bearer {{adminToken}}"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"action\": \"request_retry\",\n  \"reason\": \"Image quality was poor, resetting attempts for retry\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/registrations/{{registrationId}}/admin/review-failure",
              "host": [
                "{{baseUrl}}"
              ],
              "path": [
                "registrations",
                "{{registrationId}}",
                "admin",
                "review-failure"
              ]
            }
          }
        }
      ]
