          },
          "response": []
        },
        {
          "name": "Get Inquiry Details",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{token}}",
                "type": "text"
              }
            ],
            "url": {
              "raw": "{{base_url}}/api/listyourshow/inquiry/{{inquiryId}}",
              "host": ["{{base_url}}"],
              "path": ["api", "listyourshow", "inquiry", "{{inquiryId}}"]
            },
            "description": "Get details of a specific inquiry"
          },
          "response": []
        },
        {
          "name": "Delete Inquiry",
          "request": {
            "method": "DELETE",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{token}}",
                "type": "text"
              }
            ],
            "url": {
              "raw": "{{base_url}}/api/listyourshow/inquiry/{{inquiryId}}",
              "host": ["{{base_url}}"],
              "path": ["api", "listyourshow", "inquiry", "{{inquiryId}}"]
