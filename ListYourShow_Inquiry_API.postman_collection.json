        {
          "name": "Get My Inquiries",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{token}}",
                "type": "text"
              }
            ],
            "url": {
              "raw": "{{base_url}}/api/listyourshow/my-inquiries?page=1&limit=10",
              "host": ["{{base_url}}"],
              "path": ["api", "listyourshow", "my-inquiries"],
              "query": [
                {
                  "key": "page",
                  "value": "1"
                },
                {
                  "key": "limit",
                  "value": "10"
                }
              ]
            },
